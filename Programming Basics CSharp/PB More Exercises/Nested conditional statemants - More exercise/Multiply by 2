using System;

namespace SimpleOperationsAndMoreCalculationsMoreExercise
{
    class Program
    {
        static void Main(string[] args)
        {
            double result = 0.00;

            while (true)
            {
                double number = double.Parse(Console.ReadLine());
                if (number >= 0)
                {
                    result = number * 2;
                    Console.WriteLine($"Result: {result:F2}");
                }
                if (number<0)
                {
                    Console.WriteLine("Negative number!");
                    break;
                }
            }
        }
    }
}