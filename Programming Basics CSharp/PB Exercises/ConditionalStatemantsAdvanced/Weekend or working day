using System;

namespace ConditionalStatemantssLab
{
    class Program
    {
        static void Main(string[] args)
        {
            string dayOfWeek = Console.ReadLine();

            switch (dayOfWeek)
            {
                case "Monday":
         
                case "Tuesday":
                
                case "Wednesday":
                  
                case "Thursday":
                    
                case "Friday":
                    Console.WriteLine("Working day");
                    break;
                case "Saturday":
                    Console.WriteLine("Weekend");
                    break;
                case "Sunday":
                    Console.WriteLine("Weekend");
                    break;
                default:
                    Console.WriteLine("Error");
                        break;
            }
        }
    }
}
