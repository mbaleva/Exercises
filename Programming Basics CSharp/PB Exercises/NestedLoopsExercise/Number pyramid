using System;


namespace NestedLoopsLab
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());

            int current = 1;
            

            for (int rows = 1; rows <=n; rows++)
            {
                for (int colums = 1; colums <=rows; colums++)
                {
                    if (current>n)
                    {
                        Environment.Exit(0);
                    }
                    Console.Write(current + " ");
                    current++;

                }
                if (current > n)
                {
                    Environment.Exit(0);
                }
                Console.WriteLine();
            }
        }
    }
}

        
   