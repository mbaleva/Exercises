using System;
using System.Reflection.Metadata;

namespace PetShop
{
    class Program
    {
        static void Main(string[] args)
        {
            int dog = int.Parse(Console.ReadLine());
            int notDog = int.Parse(Console.ReadLine());
            double payDog = dog * 2.50;
            double payNotDog = notDog * 4;
            double sum = payDog + payNotDog;
            Console.WriteLine($"{ sum} lv.");
            
        }
    }
}
